    "build": "ng build",
    "build:production": "ng build --configuration production",
    "start": "ng serve --port 4000",
    "start:production": "ng serve --configuration production",
    "format": "prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}'",
    "format:fix": "prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test": "ng test",
    "test:headless": "ng test --no-watch --no-progress --browsers=ChromeHeadless"
