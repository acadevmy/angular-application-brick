{
  "name": "{{applicationName.paramCase()}}",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "dev": "ng serve",
    "start": "ng serve --configuration production",
    "build": "ng build && find dist/browser -mindepth 1 -maxdepth 1 -exec mv {} dist/browser/.. \\; && rmdir dist/browser",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,html}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx,html}' --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules pnpm jest --passWithNoTests",
    "test:headless": "NODE_OPTIONS=--experimental-vm-modules pnpm jest --silent --coverage  --passWithNoTests"
  },
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@larscom/ngx-translate-module-loader": "^5.0.0",
    "@ngrx/component-store": "^21.0.0",
    "@ngrx/effects": "^21.0.0",
    "@ngrx/store": "^21.0.0",
    "@ngrx/store-devtools": "^21.0.0",
    "@ngx-translate/core": "^16.0.4",
    "@sentry/angular": "^9.0.0",
    "lodash-es": "^4.17.21",
    "ngx-fastboot": "^3.0.0",
    "ngxtension": "^5.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@devmy/eslint-plugin": "^1.1.0",
    "@dotenv-run/core": "^1.3.6",
    "@dotenv-run/jest-angular": "^0.2.0",
    "@ngx-env/builder": "^21.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "autoprefixer": "^10.4.20",
    "esbuild": "0.24.2",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^15.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.7.0"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "executionEnv": {
      "nodeVersion": "22.12.0"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "default",
          "environment"
        ]
      }
    }
  }
}
