    "@angular/router": "^18.1.0",
    "@larscom/ngx-translate-module-loader": "^3.2.0",
    "@ngrx/component-store": "^18.0.1",
    "@ngrx/effects": "^18.0.1",
    "@ngrx/eslint-plugin": "^18.0.1",
    "@ngrx/store": "^18.0.1",
