{
  "name": "{{applicationName.paramCase()}}",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "dev": "ng serve",
    "start": "ng serve --configuration production",
    "build": "ng build",
    "format": "prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}'",
    "format:fix": "prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test": "jest --watch",
    "test:headless": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.9",
    "@angular/common": "^17.3.9",
    "@angular/compiler": "^17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/forms": "^17.3.9",
    "@angular/platform-browser": "^17.3.9",
    "@angular/platform-browser-dynamic": "^17.3.9",
    "@angular/router": "^17.3.9",
    "@ngrx/component-store": "^17.2.0",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/eslint-plugin": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@ngrx/store-devtools": "^17.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@larscom/ngx-translate-module-loader": "^3.2.0",
    "lodash-es": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "ngxtension": "^3.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/builder": "17.4.1",
    "@angular-eslint/eslint-plugin": "17.4.1",
    "@angular-eslint/eslint-plugin-template": "17.4.1",
    "@angular-eslint/schematics": "17.4.1",
    "@angular-eslint/template-parser": "17.4.1",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.9",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.4",
    "@types/jest": "^29.5.12",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0"
  },
  "engines": {
    "node": ">=20.13.1",
    "pnpm": ">=9.1.1"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": ["default", "environment"]
      }
    }
  }
}
